{
  "name": "flamingo",
  "version": "1.5.0",
  "description": "simple, hapijs based, HTTP server that allows you to convert media files to images",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "typecheck": "if [[ $(command -v flow) ]] ; then { flow check >&2; } ; else { echo \"flow command not available, skipping type checking\" >&2; } ; fi",
    "lint": "node node_modules/eslint/bin/eslint.js index.js config.js test src targets",
    "predocs": "rm -r docs 2>/dev/null || true",
    "docs": "node node_modules/jsdoc/jsdoc.js index.js config.js src -r -u tutorials -t ./node_modules/jsdoc-baseline -P package.json -R README.md -d docs",
    "pretest": "npm run lint && npm run typecheck",
    "test": "TEST=true npm run cover",
    "preversion": "npm test",
    "bench": "node targets/bench",
    "formats": "node targets/formats",
    "precover": "rm -r coverage 2>/dev/null || true",
    "cover": "node node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha",
    "postcover": "node node_modules/istanbul/lib/cli.js check-coverage"
  },
  "author": "Christian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/piobyte/flamingo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/piobyte/flamingo"
  },
  "dependencies": {
    "boom": "^2.10.1",
    "bunyan": "^1.4.0",
    "clamp": "^1.0.1",
    "fluent-ffmpeg": "fluent-ffmpeg/node-fluent-ffmpeg.git#a415251fc5008118046ae74a61f72b61cc29c38a",
    "gm": "^1.21.1",
    "hapi-lts": "hapijs/hapi.git#69eb13937875d09ce5560138fa18f6458ecf499b",
    "lodash": "^4.0.1",
    "mimeparse": "^0.1.4",
    "mkdirp": "^0.5.0",
    "request": "^2.67.0",
    "rsvp": "^3.0.18",
    "sharp": "0.12.2",
    "temp": "^0.8.3",
    "through2": "^2.0.0"
  },
  "devDependencies": {
    "aws-sdk": "^2.1.39",
    "benchmark": "^2.0.0",
    "eslint": "^1.10.3",
    "istanbul": "^0.4.1",
    "jsdoc": "^3.4.0",
    "jsdoc-baseline": "https://github.com/hegemonic/baseline/tarball/master",
    "mocha": "^2.3.4",
    "nock": "^2.18.2",
    "node-uuid": "^1.4.7",
    "sinon": "^1.17.2"
  },
  "optionalDependencies": {
    "memwatch-next": "github:marcominetti/node-memwatch"
  }
}
